  "scripts": {
    "ng": "node --max-old-space-size=8192 ./node_modules/nx/bin/nx",
    "postinstall": "node --max-old-space-size=8192 ./decorate-angular-cli.js",
    "start:frontend": "node --max-old-space-size=8192 ./node_modules/nx/bin/nx run frontend:serve:development --port=4200 --host 0.0.0.0 --poll 500",
    "build:frontend": "node --max-old-space-size=8192 ./node_modules/nx/bin/nx build frontend:build:production --prod --aot",
    "test:all": "node --max-old-space-size=8192 ./node_modules/nx/bin/nx run-many --target=test --all",
    "test:frontend": "node --max-old-space-size=8192 ./node_modules/nx/bin/nx run-many --target=test --projects=frontend",
    "test-e2e:all": "node --max-old-space-size=8192 ./node_modules/nx/bin/nx run-many --target=e2e --all",
    "test-e2e:frontend": "node --max-old-space-size=8192 ./node_modules/nx/bin/nx e2e frontend-e2e",
    "lint": "node --max-old-space-size=8192 ./node_modules/nx/bin/nx run-many --all --target=lint",
    "lint:fix": "node --max-old-space-size=8192 ./node_modules/nx/bin/nx run-many --all --target=lint --fix=true",
    "stylelint": "node --max-old-space-size=8192 ./node_modules/stylelint/bin/stylelint apps/**/*.scss --config .stylelintrc",
    "stylelint:fix": "node --max-old-space-size=8192 ./node_modules/stylelint/bin/stylelint apps/**/*.scss --fix=true --config .stylelintrc",
    "prettier": "node --max-old-space-size=8192 ./node_modules/prettier/bin-prettier .prettierrc --config .prettierrc",
    "prettier:fix": "node --max-old-space-size=8192 ./node_modules/prettier/bin-prettier --write=true .prettierrc --config .prettierrc",
    "prepare": "husky install",
    "db:start:development": "docker-compose -f docker-compose.yml -f develop.docker-compose.yml up --build",
    "db:start:prod": "docker-compose -f docker-compose.yml -f prod.docker-compose.yml up --build",
    "db:stop": "docker-compose stop",
    "server": "json-server --watch server/db.json --port 3333"
  },
