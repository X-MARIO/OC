{
	"extends": ["../../../../.eslintrc.json"],
	"ignorePatterns": ["!**/*"],
	"overrides": [
		{
			"files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
			"rules": {
				"prettier/prettier": "off",
				"@typescript-eslint/naming-convention": "off",
				"@typescript-eslint/no-namespace": "off",
				"@typescript-eslint/sort-type-constituents": "off"
			}
		},
		{
			"files": ["*.ts", "*.tsx"],
			"rules": {}
		},
		{
			"files": ["*.js", "*.jsx"],
			"rules": {}
		}
	]
}
